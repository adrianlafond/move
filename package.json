{
  "name": "move",
  "version": "0.1.0",
  "description": "App to publish move reminders",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open",
    "test": "jest --coverage",
    "test:watch": "jest --watch --coverage",
    "build": "webpack --config webpack.config.prod.js",
    "predeploy:": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrianlafond/move.git"
  },
  "keywords": [
    "move"
  ],
  "author": "Adrian Lafond <mail@adrianlafond.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adrianlafond/move/issues"
  },
  "homepage": "https://github.com/adrianlafond/move#readme",
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@types/page": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.1.3",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "modern-css-reset": "^1.1.0",
    "page": "^1.11.5",
    "preact": "^10.3.0",
    "typicons.font": "^2.0.9"
  }
}
